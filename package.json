    "sacn",
    "web",
    "protocol"
  ],
  "author": "The SHIFTY Team",
  "bugs": {
    "url": "https://github.com/oshifty/oshifty.github.io/issues"
  },
  "homepage": "https://oshifty.vision",
  "packageManager": "yarn@4.5.0"
