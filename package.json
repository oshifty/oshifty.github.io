{
  "name": "@oshifty/website",
  "version": "0.1.1",
  "description": "SHIFTY - Reinventing Fixture Control",
  "main": "index.js",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "alpinejs": "^3.14.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.17"
  },
  "scripts": {
    "start": "yarn tw && concurrently \"yarn watch:tw\" \"yarn watch:hugo\"",
